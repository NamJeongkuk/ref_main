{
  "editor.tabSize": 3,
  "editor.insertSpaces": true,
  "editor.formatOnSave": true,
  "C_Cpp.exclusionPolicy": "checkFilesAndFolders",
  "C_Cpp.files.exclude": {
    "**/ErdXMacroUtils*.h": true,
    "lib/rockhopper-boot-loader/**/*": true
  },
  "[javascript]": {
    "editor.tabSize": 2,
    "editor.insertSpaces": true
  },
  "[json]": {
    "editor.quickSuggestions": {
      "strings": true
    },
    "editor.suggest.insertMode": "replace",
    "editor.tabSize": 2,
    "editor.insertSpaces": true
  },
  "files.associations": {
    "nonvolatileasyncdatasource.h": "c",
    "fsm.h": "c"
  }
}
