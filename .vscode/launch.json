  "version": "0.2.0",
  "configurations": [{
      "name": "TDD",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/test/Build/multi-target-starter-kit_tests",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "linux": {
        "MIMode": "gdb"
      },
      "osx": {
        "MIMode": "lldb"
      },
      "preLaunchTask": "tdd_vscode_debug",
    },
    {
      "name": "rx130",
      "cwd": "${workspaceRoot}",
      "request": "launch",
      "type": "cortex-debug",
      "executable": "${workspaceFolder}/build/rx130/rx130.elf",
      "device": "R5F51308",
      "servertype": "jlink",
      "interface": "FINE",
      "jlinkscript": "${workspaceFolder}/build/rx130/debug.jlink",
      "armToolchainPath": "${workspaceFolder}/tools/kpit-rx/tc/bin/",
      "serverpath": "${workspaceFolder}/build/rx130/jlink_gdb_server_wrapper.sh",
      "preLaunchTask": "rx130_vscode_debug",
      "svdFile": "${workspaceFolder}/tools/kpit-rx/svd/rx130.svd"
    },
    {
      "name": "rx231",
      "cwd": "${workspaceRoot}",
      "request": "launch",
      "type": "cortex-debug",
      "executable": "${workspaceFolder}/build/truk/truk.elf",
      "device": "R5F52318",
      "servertype": "jlink",
      "interface": "FINE",
      "jlinkscript": "${workspaceFolder}/build/truk/debug.jlink",
      "armToolchainPath": "${workspaceFolder}/tools/kpit-rx/tc/bin/",
      "serverpath": "${workspaceFolder}/build/truk/jlink_gdb_server_wrapper.sh",
      "preLaunchTask": "truk_vscode_debug",
      "svdFile": "${workspaceFolder}/tools/kpit-rx/svd/rx231.svd"
    },
    {
      "name": "stm8s",
      "cwd": "${workspaceRoot}",
      "executable": "${workspaceFolder}/build/stm8s/stm8s-debug.elf",
      "request": "launch",
      "type": "cortex-debug",
      "servertype": "openocd",
      "runToMain": true,
      "armToolchainPath": "${workspaceFolder}/tools/sdcc-stm8/stm8-gdb/bin",
      "serverpath": "${workspaceFolder}/tools/sdcc-stm8/openocd/bin/openocd",
      "configFiles": [
        "${workspaceFolder}/build/stm8s/stm8s_debug_openocd.cfg"
      ],
      "preLaunchTask": "stm8s_vscode_debug",
      "svdFile": "${workspaceFolder}/tools/sdcc-stm8/svd/STM8Sx03.svd"
    },
    {
      "name": "stm8s-cap-touch",
      "cwd": "${workspaceRoot}",
      "executable": "${workspaceFolder}/build/stm8s-cap-touch/stm8s-cap-touch-debug.elf",
      "request": "launch",
      "type": "cortex-debug",
      "servertype": "openocd",
      "runToMain": true,
      "armToolchainPath": "${workspaceFolder}/tools/sdcc-stm8/stm8-gdb/bin",
      "serverpath": "${workspaceFolder}/tools/sdcc-stm8/openocd/bin/openocd",
      "configFiles": [
        "${workspaceFolder}/build/stm8s-cap-touch/stm8s-cap-touch_debug_openocd.cfg"
      ],
      "preLaunchTask": "stm8s-cap-touch_vscode_debug"
    },
    {
      "name": "stm32f3",
      "cwd": "${workspaceRoot}",
      "request": "launch",
      "type": "cortex-debug",
      "executable": "${workspaceFolder}/build/stm32f3/stm32f3.elf",
      "device": "STM32F303RB",
      "servertype": "jlink",
      "interface": "swd",
      "armToolchainPath": "${workspaceFolder}/tools/gcc-arm-none-eabi/tc/bin/",
      "serverpath": "${workspaceFolder}/build/stm32f3/jlink_gdb_server_wrapper.sh",
      "preLaunchTask": "stm32f3_vscode_debug",
      "svdFile": "${workspaceFolder}/lib/stm32-standard-peripherals/STM32F3xx/Libraries/CMSIS/SVD/STM32F303x.svd"
    },
    {
      "name": "rx130-cap-touch-client",
      "cwd": "${workspaceRoot}",
      "request": "launch",
      "type": "cortex-debug",
      "executable": "${workspaceFolder}/build/rx130-cap-touch-client/rx130-cap-touch-client.elf",
      "device": "R5F51308",
      "servertype": "jlink",
      "interface": "FINE",
      "jlinkscript": "${workspaceFolder}/build/rx130-cap-touch-client/debug.jlink",
      "armToolchainPath": "${workspaceFolder}/tools/kpit-rx/tc/bin/",
      "serverpath": "${workspaceFolder}/build/rx130-cap-touch-client/jlink_gdb_server_wrapper.sh",
      "preLaunchTask": "rx130-cap-touch-client_vscode_debug",
      "svdFile": "${workspaceFolder}/tools/kpit-rx/svd/rx130.svd"
    }
  ]
}
