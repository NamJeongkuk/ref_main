{
   "configurations": [
      {
         "args": [
            "--config=${workspaceFolder}/test/Tests/Integration/support/jasmine.json"
         ],
         "cwd": "${workspaceRoot}",
         "name": "Integration Test",
         "program": "${workspaceFolder}/node_modules/.bin/jasmine",
         "request": "launch",
         "runtimeVersion": "18.3.0",
         "type": "node"
      },
      {
         "args": [
            "--config=${workspaceFolder}/test/Tests/Integration/support/jasmine.json",
            "${file}"
         ],
         "cwd": "${workspaceRoot}",
         "name": "Integration Test Current File",
         "program": "${workspaceFolder}/node_modules/.bin/jasmine",
         "request": "launch",
         "runtimeVersion": "18.3.0",
         "type": "node"
      },
      {
         "args": {},
         "cwd": "${workspaceFolder}",
         "environment": {},
         "externalConsole": false,
         "linux": {
            "MIMode": "gdb"
         },
         "name": "TDD",
         "osx": {
            "MIMode": "lldb"
         },
         "preLaunchTask": "tdd_vscode_debug",
         "program": "${workspaceFolder}/test/Build/multi-target-starter-kit_tests",
         "request": "launch",
         "stopAtEntry": false,
         "type": "cppdbg"
      },
      {
         "armToolchainPath": "${workspaceFolder}/tools/kpit-rx/tc/bin",
         "cwd": "${workspaceRoot}",
         "device": "R5F51308",
         "executable": "${workspaceFolder}/build/rx130/rx130.elf",
         "interface": "FINE",
         "jlinkscript": "build/rx130/debug.jlink",
         "name": "rx130",
         "preLaunchTask": "rx130_vscode_debug",
         "request": "launch",
         "runToEntryPoint": "main",
         "serverArgs": [
            "-speed",
            "1000"
         ],
         "serverpath": "${workspaceFolder}/build/rx130/JLinkGDBServer",
         "servertype": "jlink",
         "svdFile": "tools/kpit-rx/svd/rx130.svd",
         "type": "cortex-debug"
      }
   ],
   "version": "0.2.0"
}
