{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [{
      "name": "Dugnutt launch",
      "cwd": "${workspaceRoot}",
      "request": "launch",
      "type": "cortex-debug",
      "executable": "${workspaceFolder}/build/dugnutt/dugnutt.elf",
      "device": "R5F51308",
      "servertype": "jlink",
      "interface": "FINE",
      "jlinkscript": "${workspaceFolder}/build/dugnutt/debug.jlink",
      "armToolchainPath": "${workspaceFolder}/tools/kpit-rx/tc/bin/",
      "serverpath": "${workspaceFolder}/build/dugnutt/jlink_gdb_server_wrapper.sh",
      "preLaunchTask": "dugnutt_vscode_debug"
    },
    {
      "name": "Truk launch",
      "cwd": "${workspaceRoot}",
      "request": "launch",
      "type": "cortex-debug",
      "executable": "${workspaceFolder}/build/truk/truk.elf",
      "device": "R5F52318",
      "servertype": "jlink",
      "interface": "FINE",
      "jlinkscript": "${workspaceFolder}/build/truk/debug.jlink",
      "armToolchainPath": "${workspaceFolder}/tools/kpit-rx/tc/bin/",
      "serverpath": "${workspaceFolder}/build/truk/jlink_gdb_server_wrapper.sh",
      "preLaunchTask": "truk_vscode_debug"
    },
    {
      "name": "Bonzalez launch",
      "cwd": "${workspaceRoot}",
      "executable": "${workspaceFolder}/build/bonzalez/bonzalez-debug.elf",
      "request": "launch",
      "type": "cortex-debug",
      "servertype": "openocd",
      "runToMain": true,
      "armToolchainPath": "${workspaceFolder}/tools/sdcc-stm8/stm8-gdb/bin",
      "serverpath": "${workspaceFolder}/tools/sdcc-stm8/openocd/bin/openocd",
      "configFiles": [
        "${workspaceFolder}/build/bonzalez/bonzalez_debug_openocd.cfg"
      ],
      "preLaunchTask": "bonzalez_vscode_debug"
    }
  ]
}
